<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.the.dao.ReceptionMapper">
    <select id="getPassgerFlow" resultType="int">
        select count( distinct ticketID)
        from reception
        where timeDate=#{date}
    </select>
    <insert id="createReception" parameterType="Reception" >
        insert into reception
        values(#{timeDate},#{timeClock},#{ticketID},#{scenicID},#{employeeID},0);
    </insert>
</mapper>