<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.the.dao.ScenicMapper">
    <select id="getScenic" resultType="HashMap">
        select scenicID,scenicName from scenic
        where isOnService=1;
    </select>
</mapper>